me":null,"original":{"line":32,"column":23},"generated":{"line":193,"column":36}},{"source":"scss\\styles.scss","name":null,"original":{"line":202,"column":4},"generated":{"line":197,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":202,"column":4},"generated":{"line":197,"column":13}},{"source":"scss\\styles.scss","name":null,"original":{"line":203,"column":4},"generated":{"line":198,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":203,"column":12},"generated":{"line":198,"column":14}},{"source":"scss\\styles.scss","name":null,"original":{"line":206,"column":0},"generated":{"line":201,"column":0}},{"source":"scss\\styles.scss","name":null,"original":{"line":208,"column":4},"generated":{"line":202,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":208,"column":16},"generated":{"line":202,"column":27}},{"source":"scss\\styles.scss","name":null,"original":{"line":209,"column":4},"generated":{"line":203,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":209,"column":16},"generated":{"line":203,"column":14}},{"source":"scss\\styles.scss","name":null,"original":{"line":210,"column":3},"generated":{"line":204,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":210,"column":3},"generated":{"line":204,"column":20}},{"source":"scss\\styles.scss","name":null,"original":{"line":206,"column":0},"generated":{"line":206,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":211,"column":6},"generated":{"line":206,"column":20}},{"source":"scss\\styles.scss","name":null,"original":{"line":212,"column":4},"generated":{"line":207,"column":2}},{"source":"scss\\styles.scss","name":null,"original":{"line":212,"column":4},"generated":{"line":207,"column":17}}],"sources":{"scss\\_variables.scss":"//---------- FONTS -----------//\n@import url('https://fonts.googleapis.com/css2?family=Dancing+Script:wght@500&family=Inconsolata:wght@900&family=Indie+Flower&family=Kirang+Haerang&family=Libre+Barcode+128+Text&family=Philosopher:wght@700&family=Roboto&family=Viaoda+Libre&display=swap');\n\n$font-heading: 'Philosopher', sans-serif;\n$font-body: 'Roboto', sans-serif;\n\n// Libre Barcode 128 Text\n$font-select-1: 'Libre Barcode 128 Text', cursive;\n\n// Indie Flower\n$font-select-2: 'Indie Flower', cursive;\n\n// Dancing Script:\n$font-select-3: 'Dancing Script', cursive;\n\n// Kirang Haerang\n$font-select-4: 'Kirang Haerang', cursive;\n\n// Inconsolata\n$font-select-5: 'Inconsolata', monospace;\n\n// Viaoda Libre\n$font-select-6: 'Viaoda Libre', cursive;\n\n\n//---------- COLOR PALETTE -----------//\n\n// Space Cadet\n$color-dark: #22223B;\n\n// Independence\n$color-primary: #4A4E69;\n\n// Heliotrope Gray\n$color-neutral: #9A8C98;\n\n// Silver Pink\n$color-secondary: #C9ADA7;\n\n// Isabelline\n$color-light: #F2E9E4;\n\n@mixin button {\n    border: 1px solid black;\n    padding: 1rem;\n    border-radius: 10px;\n    text-decoration: none;\n    color: $color-light;\n    background-color: $color-primary;\n}\n","scss\\styles.scss":"@import 'variables';\n\n* {\n  box-sizing: border-box;\n}\nbody {\n  margin: 0;\n  font-family: $font-body;\n  background-color: $color-light;\n\n  h1, h2, h3, h4, h5, h6 { font-family: $font-heading;}\n  \n  main {\n    margin: 10rem auto;\n    width: 50%;\n\n    #qotd, #random-quote, #favContainer {\n      display: flex;\n      flex-direction: column;\n      margin: auto;\n      padding: 0 1rem;\n      border: 1px dashed $color-primary;\n      border-radius: 1rem;\n      width: 90%;\n      background-color: whitesmoke;\n\n      #content, .content { \n        text-indent: 1.5rem; \n        font-size:x-large;\n        font-family: $font-select-3;\n        border-radius: 1rem;\n        color: $color-dark;\n        margin: 2rem .5rem 0 1rem;\n      }\n\n      #author, .author { \n        align-self: flex-end;\n        font-weight: bold;\n        color: $color-dark;\n        margin-top: 1.5rem;\n      }\n      \n    }\n  }\n\n  @media screen and (max-width: 600px) {\n    main {\n      width: 80%;\n    }\n  }\n}\n\n.topnav {\n  overflow: hidden;\n  background-color: $color-dark;\n\n  a {\n    float: left;\n    display: block;\n    color: $color-neutral;\n    text-align: center;\n    padding: 14px 16px;\n    text-decoration: none;\n    font-size: 17px;\n  }\n\n  a:hover {\n    background-color: $color-primary;\n    color: $color-light;\n  }\n\n  a.active {\n    background-color: $color-primary;\n    color: azure;\n  }\n\n  .icon {\n    display: none;\n  }\n}\n\n@media screen and (max-width: 600px) {\n  .topnav {\n      a:not(:first-child) {display: none;}\n      a.icon {\n        float: right;\n        display: block;\n        color: $color-light;\n      }\n  }\n\n  .topnav.responsive {\n    position: relative;\n    .icon {\n      position: absolute;\n      right: 0;\n      top: 0;\n      color: $color-light;\n    }\n    .icon:hover {\n      color: $color-light;\n      background-color: $color-dark;\n    }\n    a {\n      float: none;\n      display: block;\n      text-align: left;\n    }\n  }\n}\n\n// Forms \n\n.container {\n\n  border-radius: 5px;\n  background-color: $color-dark;\n  padding: 20px;\n  color: azure;\n\n  input[type=text], select, textarea {\n    width: 100%;\n    padding: 12px;\n    border: 1px solid $color-neutral;\n    border-radius: 4px;\n    resize: vertical;\n  }\n  \n  label {\n    padding: 12px 12px 12px 0;\n    display: inline-block;\n  }\n  \n  input[type=submit] {\n    background-color: $color-neutral;\n    color: azure;\n    padding: 12px 20px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n    float: right;\n  }\n  \n  input[type=submit]:hover {\n    background-color: #45a049;\n  }\n  \n  .col-25 {\n    float: left;\n    width: 25%;\n    margin-top: 6px;\n  }\n  \n  .col-75 {\n    float: left;\n    width: 75%;\n    margin-top: 6px;\n  }\n  \n  /* Clear floats after the columns */\n  .row:after {\n    content: \"\";\n    display: table;\n    clear: both;\n  }\n  \n  /* Responsive layout - when the screen is less than 600px wide, make the two columns stack on top of each other instead of next to each other */\n  @media screen and (max-width: 600px) {\n    .col-25, .col-75, input[type=submit] {\n      width: 100%;\n      margin-top: 0;\n    }\n  }\n}\n\n.search {\n  input[type=text] {\n    width: 130px;\n    box-sizing: border-box;\n    border: 2px solid $color-light;\n    border-radius: 4px;\n    font-size: 16px;\n    background-color: $color-dark;\n    background-image: url('../img/search_icon.png');\n    background-position: 10px 10px; \n    background-repeat: no-repeat;\n    padding: 12px 20px 12px 40px;\n    transition: width 0.4s ease-in-out;\n  }\n  \n  input[type=text]:focus {\n    width: 100%;\n    color: azure;\n  }\n}\n\n.btn {\n    background-color: $color-primary;\n    color: azure;\n    padding: 12px 20px;\n    border: none;\n    border-radius: 4px;\n    cursor: pointer;\n}\n\n#options {\n  .btn {\n    margin: 1rem;\n    float: right;\n  }\n  .btn:disabled {\n    background-color: $color-neutral;\n    cursor:not-allowed;\n  }\n}"},"lineCount":null}},"error":null,"hash":"099f2b331fccc326f7184600adc6cdad","cacheData":{"env":{}}}